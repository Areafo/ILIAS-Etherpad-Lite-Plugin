<div id="etherpad-lite" class="etherpad-liteRegular">
    <div class="labeFullscreenPad">
        <a id="enterFullscreenPad">{ENTER_FULLSCREEN}</a>
        <a id="leaveFullscreenPad">{LEAVE_FULLSCREEN}</a>
    </div>
    <iframe id = "etherpad-liteFrame" name="etherpad-liteFrame"
        src="{PROTOCOL}://{HOST}:{PORT}/p/{ETHERPADLITE_ID}?showControls={SHOW_CONTROLS}&showChat={SHOW_CHAT}&showLineNumbers={SHOW_LINE_NUMBERS}&useMonospaceFont={USE_MONOSPACE_FONT}&userName={USER_NAME}&noColors={NO_COLORS}&showStyleBlock={SHOW_STYLE_BLOCK}&showListBlock={SHOW_LIST_BLOCK}&showRedoBlock={SHOW_REDO_BLOCK}&showColorBlock={SHOW_COLOR_BLOCK}&showHeadingBlock={SHOW_HEADING_BLOCK}&showImportExportBlock={SHOW_IMPORT_EXPORT_BLOCK}&showTimelineBlock={SHOW_TIMELINE_BLOCK}&language={LANGUAGE}">
    </iframe>
</div>

